<%- include('partials/heading') -%>
    <div style="margin-top: 60px; padding-bottom:400px;" class="container text-center">
        <div class="class-center my-4">
            <h3>SignUp !</h3>
        </div>
        <form action="/register" , method="POST">
            <div class="form-group">
                <input class="form-control" type="text" name="image" placeholder="Picture">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="username" placeholder="Username">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="email" placeholder="Email">
            </div>
            <div class="form-group">
                <input class="form-control" type="password" name="password" placeholder="Password">
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-dark text-info ">Sign up</button>
            </div>
        </form>
    </div>


    <%- include('partials/footer') -%>